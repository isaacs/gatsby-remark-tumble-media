    "snap": "TAP_SNAPSHOT=1 tap test/*.js"
  },
  "files": [
    "index.js",
    "gatsby-browser.js",
    "gatsby-node.js",
    "overrides.css",
    "base.css"
  ],
  "version": "1.0.3",
  "description": "A gatsby plugin to do tumble blog style media embeds, like photosets, videos, and links.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/isaacs/gatsby-remark-tumble-media.git"
  },
